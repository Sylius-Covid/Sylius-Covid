<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="vanoix_customer_support.listener.admin.menu_builder" class="Vanoix\SyliusCustomerSupportPlugin\Menu\AdminMenuListener">
            <tag name="kernel.event_listener" event="sylius.menu.admin.main" method="addAdminMenuItems" />
        </service>

        <service id="vanoix_customer_support.form.type.support" class="Vanoix\SyliusCustomerSupportPlugin\Form\Type\SupportType">
            <argument>%vanoix_customer_support.model.support.class%</argument>
            <tag name="form.type" />
        </service>
    </services>
</container>
